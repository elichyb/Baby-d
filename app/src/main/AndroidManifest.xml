<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.elichy.baby_d">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.BabyDiaper"></activity>
        <activity android:name=".Activities.BabyWeight" />
        <activity android:name=".Activities.BabyViewActivity" />
        <activity
            android:name=".Activities.AddBabyViewActivity"
            android:label="@string/AddBabyTitle" />
        <activity
            android:name=".Activities.RegisterActivity"
            android:label="@string/RegisterTitle" />
        <activity
            android:name=".Activities.ParentViewActivity"
            android:label="@string/WelcomeTitle" />
        <activity
            android:name=".Activities.MainActivity"
            android:label="@string/LoginTitle">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>